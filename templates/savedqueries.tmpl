<html>
<head>
<SCRIPT LANGUAGE=JAVASCRIPT>
function verify() {
        msg = "Are you sure you want to delete this query?";
        return confirm(msg);
}
</SCRIPT>
<link rel="stylesheet" type="text/css" href="[[PISSROOT]]/js/ext-all.css" />
<link rel="stylesheet" type="text/css" href="[[PISSROOT]]/js/xtheme-gray.css" />

<LINK REL=stylesheet HREF="[[PISSROOT]]/traq.css" TYPE="text/css">

</HEAD>
<body>
<table cellspacing="0" cellpadding="0" border="0" width="100%">
	<TR class="form_header">
	<td width=15></td>
	<td >
		<div id="container">
    		<div id="toolbar"><a href="[[PISSROOT]]/">Projectraq</a> <a href="[[PISSROOT]]/display_query.cgi?dhtml=1&cat=bug">Find bugs</a> <a href="[[PISSROOT]]/display_query.cgi?dhtml=1&cat=task">Find tasks</a></div>
		</div>
	</td>
	<TD width=65% align=left style="height:35px;padding-left:30px;" valign="middle">
	</TD>
	</TR>
</table>

<FORM ACTION="./do_query.cgi" METHOD=POST>
<TABLE cellpadding=4 cellspacing=0 class=form_table>
<TR class=form_header>
	<TD colspan=5>
		<h3>My queries:</h3>
	</TD>
</TR>
<TR>
        <TD>
                <INPUT CHECKED TYPE=radio NAME=qname VALUE=traq_defaultquery>
        </td>
        <td colspan=4>
                <A HREF="./display_saved.cgi?qname=defaultquery&mode=viewsql">Default(Current) Query</a>
        </TD>
</TR>
[[BEGINQUERYLIST]]
<tr>
	<td>
		<input type=radio name=qname value=traq_[[QUERYNAME]]>
	</td>
	<td>
		<a href="./do_query.cgi?qname=traq_[[QUERYNAME_ESC]]">[[QUERYNAME]]</a>
	</td>
	<td>
		<A HREF="display_saved.cgi?qname=[[QUERYNAME]]&mode=viewsql">Display SQL code </a>
	</td>
	<td>
		<a href="[[QUERYURL]]">Query as URL</a>
	</td>
	<td>
		<A HREF="delete_named.cgi?qname=[[QUERYNAME_ESC]]" onClick="return verify()"><font size=-1>Delete</font></A> 
	</td>
</tr>
[[ENDQUERYLIST]]
<tr>
<td colspan=4>
<INPUT TYPE=SUBMIT onClick="this.value='Please wait...'">
</td>
</tr>
</FORM>
</TABLE>
<script src="[[PISSROOT]]/js/ext-base.js" type="text/javascript"></script>
<script src="[[PISSROOT]]/js/ext-all.js" type="text/javascript"></script>
<script type="text/javascript" src="[[PISSROOT]]/js/ptlib.js"></script>

<script>
[[HEADER]]
</script>

</body>
</htmL>